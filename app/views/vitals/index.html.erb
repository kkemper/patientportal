<h1>Listing Vitals</h1>
<div class =  "media">
 <div class = "media-body">
 <table class="table table-hover">
          <thead>
            <tr>
              <th>Date</th>
              <th>Patient ID</th>
              <th>Vital Type</th>
              <th>Vital value</th>
              <th colspan="3"></th>
            </tr>
          </thead>

          <tbody>
            <% @vitals.each do |vital| %>
              <tr>
                <td><%= vital.date %></td>
                <td><%= vital.Patient_ID %></td>
                <td><%= vital.Vital_Value %></td>
                <td><%= link_to 'Show', vital,class: 'btn btn-mini' %></td>
                <td><%= link_to 'Edit', edit_vital_path(vital),class: 'btn btn-mini' %></td>
                <td><%= link_to 'Destroy', vital, method: :delete,,class: 'btn btn-mini' ,data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
          </tbody>
        </table>

  </div>
  </div>
<br>

<%= link_to 'New Vital', new_vital_path %>
